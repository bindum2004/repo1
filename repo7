ðŸ˜‚ðŸ˜‚ðŸ˜‚ðŸ˜‚


- name: Install and run nginx on localhost
  hosts: local
  become: yes
  tasks:
    - name: Install nginx
      apt:
        name: nginx
        state: present
        update_cache: yes

    - name: Force stop all nginx processes (WSL compatible)
      shell: |
        sudo pkill -9 nginx || true
        sleep 2  # Wait for process termination
      changed_when: false
      ignore_errors: yes

    - name: Start nginx (with port conflict handling)
      shell: |
        sudo nginx -s stop || true  # Try graceful stop first
        sudo nginx
      changed_when: false
      async: 10
      poll: 2


or


- name: Install and run nginx on localhost
  hosts: local
  become: yes
  tasks:
    - name: Install nginx
      apt:
        name: nginx
        state: present
        update_cache: yes

    - name: Stop any existing nginx (cleanup)
      command: sudo pkill -9 nginx || true
      changed_when: false
      ignore_errors: yes

    - name: Start nginx (WSL compatible)
      command: sudo nginx
      changed_when: false

    - name: Verify nginx is running
      command: curl -s http://localhost
      register: nginx_status
      changed_when: false
ðŸ˜‚ðŸ˜‚
[local]
localhost ansible_connection=local



or


- name: Install and run nginx on localhost
  hosts: local
  become: yes
  tasks:
    - name: Install nginx
      apt:
        name: nginx
        state: present
        update_cache: yes

    - name: Start nginx manually
      command: nginx

